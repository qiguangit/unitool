<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            stylesheets="/css/generate.css"
            fx:controller="com.qiguangit.unitool.plugin.generate.controller.TemplateController"
            prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="tvTemplates" >
        <columns>
            <TableColumn fx:id="checkColumn" prefWidth="70.0" minWidth="40.0" maxWidth="70.0" text="生效" />
            <TableColumn fx:id="idxColumn" prefWidth="70.0" minWidth="40.0" maxWidth="70.0" text="" />
            <TableColumn fx:id="templateColumn" prefWidth="75.0" text="模板" />
            <TableColumn fx:id="commentColumn" prefWidth="75.0" text="备注" />
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>
</VBox>
